<?xml version="1.0" encoding="UTF-8"?>

<!--  This is our beans -->

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd"
>

	<context:annotation-config />

	<bean class="com.springmvc.auto.wire.annotations.Address" name="address1">
		<property name="street" value="Dalal Street" />
		<property name="city" value="Mumbai" />
	</bean>
	<bean class="com.springmvc.auto.wire.annotations.Address" name="address2">
		<property name="street" value="Chakan Road" />
		<property name="city" value="Pune" />
	</bean>
	
	<bean class="com.springmvc.auto.wire.annotations.Employee" name="emp">
		
		
		
	</bean>

</beans>