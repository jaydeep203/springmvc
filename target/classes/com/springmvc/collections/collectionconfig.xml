<?xml version="1.0" encoding="UTF-8"?>

<!--  This is our beans -->

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd"
>

	<bean name="employee1" class="com.springmvc.collections.Employee">
		<property name="name" value="Jay" />
		<property name="phones">
			<list>
				<value>+91 6866978978</value>
				<value>+91 6866978974</value>
				<value>+91 6866978975</value>
			</list>
		</property>
		<property name="addresses">
			<set>
				<value>Delhi</value>
				<value>Mumbai</value>
				<value>Pune</value>
			</set>
		</property>
		<property name="courses">
			<map>
				<entry key="Java" value="3months" />
				<entry key="python" value="5months" />
			</map>
		</property>
		
	</bean>

	
</beans>